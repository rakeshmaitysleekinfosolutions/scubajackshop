{
	"description": "The CodeIgniter framework with HMVC Library",
	"name": "alzen8work/ci_hmvc",
	"type": "project",
	"homepage": "https://github.com/alzen8work/ci_hmvc",
	"license": "MIT",
	"require": {
		"php": ">=5.2.4",
		"yidas/codeigniter-model": "^2.19",
		"nesbot/carbon": "^2.16",
		"stripe/stripe-php": "^7.51",
		"paypal/rest-api-sdk-php": "*"
	},
	"suggest": {
		"paragonie/random_compat": "Provides better randomness in PHP 5.x"
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.*",
		"phpunit/phpunit": "4.* || 5.*"
	},
	"autoload": {
        "psr-4": {
            "Mail\\": "application/mail/",
			"Application\\Contracts\\": "application/contracts/"
        }
    }
}
